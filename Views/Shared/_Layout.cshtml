                </a>

                <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarScroll" aria-controls="navbarScroll" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon fa icon-expand fa-bars"></span>
                    <span class="navbar-toggler-icon fa icon-close fa-times"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarScroll">
                    <ul class="navbar-nav me-lg-auto my-2 my-lg-0 navbar-nav-scroll flex-column flex-lg-row">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>

                        @if (User.IsInRole("Admin"))
                        {
                                <li class="nav-item">
                                    <a class="nav-link" href="/LeaveRequests/Create">Leave Request</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/StaffMembers/Index">Staff Members</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Teams/Index">Teams</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/AttendanceBonus/Index">Track AttendanceBonus</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/WorkUpdates/Index">Work Updates</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/WorkUpdateTopics/Index">Work Updates Topics</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Events/Index">Events</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/EventCategories/Index">Events Categories</a>
                                </li>
                                     <li class="nav-item">
                                    <a class="nav-link" href="/JobPostings/Create
    ">Post a Job</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="JobApplications/Index">Job Applications</a>
                                </li>
                        }
                    </ul>

                    <ul class="navbar-nav search-right mt-lg-0 mt-2">
                        <li class="nav-item mr-2" title="Search">
                            <a href="#search" class="search-search">
                                <span class="fas fa-search" aria-hidden="true"></span>
                            </a>
                        </li>
                        <li class="nav-item mx-lg-4 phone-btn btn-white d-none d-lg-block btn-style ms-2">
                            <partial name="_LoginPartial" />
                        </li>
                    </ul>

                    <!-- //toggle switch for light and dark theme -->
                    <!-- search popup -->
                    <div id="search" class="w3lpop-overlay">
                        <div class="w3lpopup">
                            <form action="#formsearch" method="GET" class="d-flex">
                                <input type="search" placeholder="Search.." name="search" required="required" autofocus>
                                <button type="submit"><span class="fas fa-search"></span></button>
                                <a class="close" href="#formsearch">&times;</a>
                            </form>
                        </div>
                    </div>
                    <!-- /search popup -->
                </div>

                <!-- Optional CSS for additional styling -->
                <style>
                    /* Flexbox adjustments for navbar items */
                    .navbar-nav {
                        display: flex;
                        flex-wrap: wrap; /* Allow the items to wrap into multiple lines */
                        margin: 0;
                    }

                        .navbar-nav .nav-item {
                            flex: 1 0 100%; /* Make each item take full width initially */
                        }

                    /* Adjust for larger screens to display items in a row */
                    @@media (min-width: 992px) {
                        .navbar-nav .nav-item

                    {
                        flex: 0 0 auto; /* Prevent wrapping and allow items to be in a row on large screens */
                    }

                    }
                </style>

                <!-- toggle switch for light and dark theme -->
                <div class="mobile-position">
                    <nav class="navigation">
                        <div class="theme-switch-wrapper">
                            <label class="theme-switch" for="checkbox">
                                <input type="checkbox" id="checkbox">
                                <div class="mode-container">
                                    <i class="gg-sun"></i>
                                    <i class="gg-moon"></i>
                                </div>
                            </label>
                        </div>
                    </nav>
                </div>
                <!-- //toggle switch for light and dark theme -->
            </nav>
        </div>
    </header>
    <!--//Header-->

    <body>
        @RenderBody()
    </body>

